{
    "name": "temp/text-extractor",
    "type": "library",
    "description": "mime detection",
    "keywords": ["text", "extract"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Wentz",
            "email": "stephan@wentz.it",
            "homepage": "http://www.wentz.it/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Temp\\TextExtractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Temp\\TextExtractor\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "temp/media-classifier": "~1.0",
        "php-poppler/php-poppler": "~1.0",
        "symfony/dom-crawler": "~2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
